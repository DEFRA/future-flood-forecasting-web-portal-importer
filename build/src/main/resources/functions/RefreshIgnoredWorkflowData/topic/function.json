{
  "bindings": [
    {
      "name": "message",
      "type": "serviceBusTrigger",
      "direction": "in",
      "topicName": "fews-ignored-workflows-topic",
      "subscriptionName": "%AZURE_SERVICE_BUS_IGNORED_WORKFLOWS_SUBSCRIPTION_NAME%",
      "connection": "AzureWebJobsServiceBus"
    },
    {
      "name": "processFewsEventCode",
      "type": "serviceBus",
      "direction": "out",
      "topicName": "fews-eventcode-topic",
      "subscriptionName": "%AZURE_SERVICE_BUS_EVENT_CODE_SUBSCRIPTION_NAME%",
      "connection": "AzureWebJobsServiceBus"
    },
    {
      "name": "importFromFews",
      "type": "serviceBus",
      "direction": "out",
      "topicName": "fews-import-topic",
      "subscriptionName": "%AZURE_SERVICE_BUS_FEWS_IMPORT_SUBSCRIPTION_NAME%",
      "connection": "AzureWebJobsServiceBus"
    }
  ]
}
