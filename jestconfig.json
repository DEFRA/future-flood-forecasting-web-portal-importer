{
  "collectCoverage": true,
  "collectCoverageFrom": [
    "**/*.js",
    "!**/node_modules/**",
    "!**/testing/**",
    "!**/vendor/**"
  ],
  "coverageDirectory": "testing/coverage",
  "coverageReporters": [
    "text",
    "lcov"
  ],
  "coverageThreshold": {
    "global": {
      "branches": 90,
      "functions": 90,
      "lines": 90,
      "statements": 90
    }
  },
  "resetMocks": true,
  "restoreMocks": true
}
